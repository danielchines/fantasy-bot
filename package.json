{
  "name": "fantasy-bot",
  "version": "0.1.0",
  "author": {
    "name": "Danny Hines"
  },
  "bin": {
    "fantasy-bot": "bin/fantasy-bot.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
