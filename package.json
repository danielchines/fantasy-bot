    "update-event": "sam local invoke -e test/update-event.json -t ./cdk.out/FantasyBotStack.template.json FantasyBot"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.100.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
