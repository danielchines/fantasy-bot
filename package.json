    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7",
    "dotenv": "^10.0.0"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.100.0",
    "@aws-cdk/aws-events": "1.100.0",
    "@aws-cdk/aws-events-targets": "1.100.0",
    "@aws-cdk/aws-lambda": "1.100.0",
